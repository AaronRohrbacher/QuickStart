<%= form_for @company do |f| %>
  <%=f.label :name%>
  <%=f.text_field :name %>
  <%=f.label :address_1%>
  <%=f.text_field :address_1 %>
  <%=f.label :address_2 %>
  <%=f.text_field :address_2 %>
  <%=f.label :city %>
  <%=f.text_field :city %>
  <%=f.label :state %>
  <%=f.text_field :state %>
  <%=f.label :zip %>
  <%=f.text_field :zip %>
  <%=f.label :phone_1%>
  <%=f.text_field :phone_1 %>
  <%=f.label :phone_2%>
  <%=f.text_field :phone_2 %>
  <%=f.submit "Add Company", class: 'btn btn-success' %>
<% end %>
